{% extends 'base.html.twig' %}

{% block title %}Carnet d'adresses
{% endblock %}

{% block body %}
	<div class="container m-4">
		{% for contact in contacts %}
			<div class="card shadow m-2 p-4">
				<div class="body">
					<h5 class="card-title">
						{{ contact.nom}}
						{{ contact.prenom }}
					</h5>
					<p class="card-text">
						{{ contact.telephone }}
					</p>
				</div>
			</div>
		{% endfor %}
	</div>
{% endblock %}
